# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/migration_db
MONGODB_URI=mongodb://localhost:27017/migration_tracker

# API Keys
CENSUS_API_KEY=your_census_api_key_here
MIGRATION_DATA_API_KEY=your_migration_data_api_key_here

# File Paths
DATA_DIR=./data
REPORTS_DIR=./reports
LOGS_DIR=./logs

# Application Settings
DEBUG=True
LOG_LEVEL=INFO

# External Data Sources
EXTERNAL_DATA_URL=https://api.example.com/migration-data
BACKUP_DATA_URL=https://backup.example.com/data

# Notification Settings
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USER=your_email@example.com
EMAIL_PASSWORD=your_email_password
